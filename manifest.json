{
  "manifest_version": 2,
  "name": "Offline Reader",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": ["storage", "alarms", "notifications", "webview", "<all_urls>"],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}